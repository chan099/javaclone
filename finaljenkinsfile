node{
  stage('scm checkout'){
    git 'https://github.com/sandeep212/javaclone.git'
  }
  stage('mvn buid'){
    sh 'mvn package'
  }
  stage('email notifications'){
    mail bcc: '', 
    body: 'job build sucessfully', 
    cc: '', from: '', 
    replyTo: '', 
    subject: 'from jenkins', 
    to: 'sandeepchowdary212@gmail.com'
  }
}
